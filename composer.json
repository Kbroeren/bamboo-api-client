{
  "name": "steffenbrand/bamboo-api-client",
  "description": "Bamboo API Client",
  "homepage": "https://github.com/steffenbrand/bamboo-api-client",
  "type": "library",
  "version": "1.0.1",
  "authors": [
    {
      "name": "Steffen Brand",
      "email": "s.brand@steffenbrand.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "SteffenBrand\\BambooApiClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SteffenBrand\\BambooApiClient\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
